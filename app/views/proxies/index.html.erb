<h1>Listing proxies</h1>

<table>
  <thead>
    <tr>
      <th>Proxy Address</th>
      <th>Proxy Port</th>
      <th>Proxy Status</th>
      <th>Proxy Timeout</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @proxies.each do |proxy| %>
      <tr>
        <td><%= proxy.proxy_address %></td>
        <td><%= proxy.proxy_port %></td>
        <td><%= proxy.proxy_status %></td>
        <td><%= proxy.proxy_timeout %></td>
        <td><%= link_to 'Show', proxy %></td>
        <td><%= link_to 'Edit', edit_proxy_path(proxy) %></td>
        <td><%= link_to 'Destroy', proxy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proxy', new_proxy_path %>
